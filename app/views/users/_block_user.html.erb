<% blocks.each do |user| %>
  <br>
  <div id="user-<%= user.id %>" class="row">
    <div class="col-lg-8">
      <%= render 'shared/avatar', user: user %>
      <h5 class="name"><%= link_to user.name, user %></h5>
    </div>
    <div class="col-lg-4">
      <div class="follow">
        <%= render 'users/block_button', user: user %>
      </div>
    </div>
  </div>
  <br>
<% end %>


